<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Aluno.
    -->
    <changeSet id="20171012224601-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="artigo_id"
                                 baseTableName="aluno"
                                 constraintName="fk_aluno_artigo_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artigo"/>

        <addForeignKeyConstraint baseColumnNames="proposta_tese_id"
                                 baseTableName="aluno"
                                 constraintName="fk_aluno_proposta_tese_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proposta_tese"/>

        <addForeignKeyConstraint baseColumnNames="co_orientador_id"
                                 baseTableName="aluno"
                                 constraintName="fk_aluno_co_orientador_id"
                                 referencedColumnNames="id"
                                 referencedTableName="co_orientador"/>

    </changeSet>
</databaseChangeLog>
